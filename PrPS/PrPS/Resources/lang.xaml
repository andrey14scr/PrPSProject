                    xmlns:system="clr-namespace:System;assembly=System.Runtime">

    <system:String x:Key="Title">Digital registry of rooms</system:String>
    <system:String x:Key="Login">Login</system:String>
    <system:String x:Key="Password">Password</system:String>
    <system:String x:Key="DontHaveAccount">Don't have account?</system:String>
    <system:String x:Key="AlreadyHaveAccount">Already have account?</system:String>
    <system:String x:Key="Register">Register</system:String>
    <system:String x:Key="SignIn">Sign in</system:String>
